        "gulp-typescript": "latest",
        "react-devtools": "latest",
        "run-sequence": "latest",
        "tslint": "latest",
        "typescript": "latest"
    },
    "peerDependencies": {
        "react": "16.7.0",
        "react-native": "^0.57.8",
